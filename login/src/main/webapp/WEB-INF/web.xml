<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
<filter>
  	<filter-name>LoginFilter</filter-name>
  	<display-name>LoginFilter</display-name>
  	<description></description>
  	<filter-class>com.bridgeLabz.controller.LoginFilter</filter-class>
</filter>
<filter-mapping>
  	<filter-name>LoginFilter</filter-name>
  	<url-pattern>/stu_login</url-pattern>
</filter-mapping>


<servlet>
  <servlet-name>register</servlet-name>
  <servlet-class>com.bridgeLabz.controller.register</servlet-class>
</servlet>  
<servlet>
  <servlet-name>login</servlet-name>
  <servlet-class>com.bridgeLabz.controller.login</servlet-class>
</servlet>
<servlet>
  <servlet-name>logout</servlet-name>
  <servlet-class>com.bridgeLabz.controller.logout</servlet-class>
</servlet>
  
  
<servlet-mapping>
  <servlet-name>register</servlet-name>
  <url-pattern>/stu_register</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/stu_login</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>logout</servlet-name>
  <url-pattern>/stu_logout</url-pattern>
</servlet-mapping>
</web-app>
